Ones and Zeros:
import java.util.List;

public class BinaryArrayToNumber {
        
    public static int ConvertBinaryArrayToInt(List<Integer> binary) {
         int len = binary.size();
         int result = 0;
         int bin = 1;
        for (int i=1; i < len+1;  i++) {
          if (binary.get(len-i) == 1)
              result += bin;
          bin = bin*2;
        }
       return result;
    }
}

Valid Braces - i'll finish it later
public class BraceChecker {

  public boolean isValid(String braces) {
  int count = 0;
  
    for (int i=0; i<braces.length(); i++) { // looking for first braces inequality every 2 chars
    
        if (!braces.chatAt(i).equals(braces.charAt(i+1)))  { // if found inequality
        
            if ((braces.length()-i)%2 > 0) //parity check of the remaining numbers
                return false;   
            
            for (int p=i+1; p<braces.length(); p++) { //looking for first equal chars, count++
                count++;
                if (braces.chatAt(p).equals(braces.charAt(p+1))){ // if found equality
                    int minus = -1;
                    int plus = 2;
                    
                    for (int o=0; o < count; o++) { //looking back count times to check that all previous braces were correct                                               
                        if (!braces.chatAt(p + minus).equals(braces.charAt(p + plus))){ //false if found inequality   
                           return false; 
                        }       
                        minus--;
                        plus++; 
                    }                    
                    
                    i = p + count;
                    p = braces.length();
                }
               
            }
           
        }  
       i++;       
    }
    return true;
  }

}